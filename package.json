{
  "name": "maia",
  "version": "0.0.1",
  "description": "Route manager",
  "scripts": {
    "start": "./node_modules/.bin/forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server/server.js",
    "dev-server": "./node_modules/.bin/webpack --watch --verbose --colors --display-error-details --config ./config/webpack.server.dev.babel.js",
    "dev-server-start": "node ./node_modules/just-wait --pattern \"./dist/server/server.js\" && npm run start",
    "dev-client": "./node_modules/.bin/webpack-dev-server --no-info --config ./config/webpack.client.dev.babel.js",
    "dev": "./node_modules/.bin/concurrently --kill-others \"npm run dev-client\" \"npm run dev-server\" \"npm run dev-server-start\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "just-wait": "^1.0.5",
    "node-reset-scss": "^1.0.1",
    "node-sass": "^3.7.0",
    "null-loader": "^0.1.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-node-externals": "^1.2.0"
  },
  "dependencies": {
    "express": "^4.13.4",
    "forever": "^0.15.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "redux": "^3.5.2"
  }
}
