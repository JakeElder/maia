{
  "name": "maia",
  "version": "0.0.1",
  "description": "Route manager",
  "repository": "git@bitbucket.org:squareenixoid/onlinedev-maia.git",
  "scripts": {
    "start": "./node_modules/.bin/forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server/server.js",
    "build-server": "NODE_ENV=production ./node_modules/.bin/webpack --display-error-details --config ./webpack.server.babel.js",
    "build-client": "NODE_ENV=production ./node_modules/.bin/webpack --display-error-details --config ./webpack.client.babel.js",
    "build": "./node_modules/.bin/concurrently \"npm run build-server\" \"npm run build-client\"",
    "dev-server": "./node_modules/.bin/webpack --watch --verbose --colors --display-error-details --config ./webpack.server.babel.js",
    "dev-server-start": "./node_modules/.bin/just-wait --pattern \"./dist/server/server.js\" && npm run start",
    "dev-client": "./node_modules/.bin/webpack-dev-server --config ./webpack.client.babel.js",
    "dev": "./node_modules/.bin/concurrently --kill-others \"npm run dev-client\" \"npm run dev-server-start\" \"npm run dev-server\"",
    "test": "NODE_ENV=test ./node_modules/.bin/jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "isomorphic-style-loader": "^1.0.0",
    "jest-cli": "^12.1.1",
    "just-wait": "^1.0.5",
    "null-loader": "^0.1.1",
    "server-destroy": "^1.0.1",
    "socket.io-client": "^1.4.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-node-externals": "^1.2.0"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "browser-cookies": "^1.0.8",
    "classnames": "^2.2.5",
    "cookie": "^0.2.4",
    "express": "^4.13.4",
    "forever": "^0.15.1",
    "form-serialize": "^0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.12.0",
    "lodash.bindall": "^4.2.1",
    "lodash.findindex": "^4.4.0",
    "lodash.isequal": "^4.2.0",
    "lodash.omit": "^4.3.0",
    "lodash.sortby": "^4.5.0",
    "material-ui": "^0.15.0",
    "nconf": "^0.8.4",
    "react": "^15.0.2",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-tap-event-plugin": "^1.0.0",
    "redis": "^2.6.0-2",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.1",
    "reset-css": "^2.0.2011012603",
    "socket.io": "^1.4.6"
  },
  "jest": {
    "verbose": true,
    "unmockedModulePathPatterns": [],
    "moduleNameMapper": {
      "route-api": "<rootDir>/src/route/api.server"
    }
  }
}
